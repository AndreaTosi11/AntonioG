@model ProjectWorkDemo.Models.Lavorazione

@{
    ViewData["Title"] = "Details";
}

<head>
	<meta charset="utf-8" />
	<title>@ViewData["Title"] - Home</title>
	<link rel="stylesheet" href="/css/details-delete.css" />
    <link rel="stylesheet" href="/css/menuEfooter.css" />
</head>

<body>
<div class="container">
<h1>@sharedResource["DettagliLavorazione"]</h1>
    <div class="form">
        <div class="formCenter">
        <div class="btl">
            <a class="nav-link-back" asp-action="Index">@sharedResource["TornaAllaLista"]</a>
        </div>
        <div class="head-body">
        <dl>
        <dt class = "col-sm-2">
            @sharedResource["CodiceIdentificativo"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodiceIdentificativo)
        </dd>
        <dt class = "col-sm-2">
            @sharedResource["DataLavorazione"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataLavorazione)
        </dd>
        <dt class = "col-sm-2">
            @sharedResource["CostoTotaleDellaLavorazione"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CostoTotaleLavorazione)
        </dd>
        <dt class = "col-sm-2">
            @sharedResource["Garanzia"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Garanzia)
        </dd>
        <dt class = "col-sm-2">
            @sharedResource["CodiceGaranzia"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NumGaranzia)
        </dd>
        <dt class = "col-sm-2">
             @sharedResource["FornitoriName"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdFornitoriNavigation.NomeDitta)
        </dd>
        <dt class = "col-sm-2">
            @sharedResource["Targa"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdMezzoNavigation.Targa)
        </dd>
        </dl>
        </div>
        <div class="btl">
            <a class="nav-link-back" asp-action="Edit" asp-route-id="@Model?.IdLavorazione">@sharedResource["Modifica"]</a>
        </div>
        </div>
    </div>
</div>
</body>
