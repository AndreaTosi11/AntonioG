@model ProjectWorkDemo.Models.ServiziFornitore

@{
    ViewData["Title"] = "Details";
}

<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] - Home</title>
    <link rel="stylesheet" href="/css/details-delete.css" />
    <link rel="stylesheet" href="/css/menuEfooter.css" />
</head>

<body>
    <div class="container">
        <h1>@sharedResource["DettagliServizi"]</h1>
        <div class="form">
            <div class="formCenter">
                <div class="btl">
                    <a class="nav-link-back" asp-action="Index">@sharedResource["TornaAllaLista"]</a>
                </div>
                <div class="head-body">
                    <dl>
                        <dt class="col-sm-2">
                            @sharedResource["NomeDitta"]
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.IdFornitoriNavigation.NomeDitta)
                        </dd>
                        <dt class="col-sm-2">
                            @sharedResource["Descrizione"]
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.IdTipoFornNavigation.Descrizione)
                        </dd>
                    </dl>
                </div>
                <div class="btl">
                    <a class="nav-link-back" asp-action="Edit" asp-route-id="@Model?.IdFornitori">@sharedResource["Modifica"]</a>
                </div>
            </div>
        </div>
    </div>
</body>